{
	"name": "@titanium/coremotion",
	"version": "1.0.7",
	"description": "Native modules that allows you to use iOS CoreMotion framework with Axway Titanium native mobile apps.",
	"main": "index.js",
	"scripts": {
		"list-files": "npm pack && tar -xvzf *.tgz && rm -rf package *.tgz",
		"postinstall": "tiapp-install-module",
		"postuninstall": "tiapp-uninstall-module",
		"test": "npm install --ignore-scripts && npm run list-files",
		"prepublishOnly": "git pull && bump"
	},
	"author": "Axway",
	"maintainers": [
		{
			"name": "Brenton House",
			"email": "brenton.house@gmail.com",
			"url": "https://brenton.house"
		}
	],
	"repository": {
		"type": "git",
		"url": "github:brentonhouse/titanium-coremotion"
	},
	"license": "Apache-2.0",
	"bugs": {
		"url": "https://github.com/brentonhouse/titanium-coremotion/issues"
	},
	"homepage": "https://github.com/brentonhouse/titanium-coremotion#readme",
	"keywords": [
		"titanium",
		"mobile",
		"alloy",
		"turbo",
		"axway",
		"appcelerator",
		"ios",
		"android",
		"iphone",
		"ipad",
		"apple",
		"coremotion",
		"titanium-native-module",
		"brenton-house"
	],
	"titanium": {
		"type": "native-module",
		"version": "1.0.5",
		"moduleid": "titanium-coremotion",
		"name": "titanium-coremotion",
		"platform": {
			"ios": {
				"apiversion": 2,
				"architectures": [
					"armv7",
					"arm64",
					"i386",
					"x86_64"
				],
				"minsdk": "5.0.0",
				"guid": "bba89061-0fdb-4ff1-95a8-02876f5601f9"
			}
		}
	},
	"dependencies": {
		"@titanium/tiapp-xml": "^1.0.4"
	}
}
